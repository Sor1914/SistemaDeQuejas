  <data name="FiltroCorrelativo" xml:space="preserve">
    <value>Encabezado_Queja.CORRELATIVO = '{0}'</value>
  </data>
  <data name="FiltroFechas" xml:space="preserve">
    <value>CONVERT(varchar(10), Encabezado_Queja.Fecha_Ingreso, 120)  &gt;= '{0}' AND CONVERT(varchar(10), Encabezado_Queja.Fecha_Ingreso, 120) &lt;= '{1}'</value>
  </data>
